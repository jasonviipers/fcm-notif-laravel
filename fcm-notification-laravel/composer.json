{
    "name": "viipers/fcm-notification-laravel",
    "description": "Laravel FCM (Firebase Cloud Messaging) Notification Channel",
    "license": "MIT",
    "keywords": [
        "laravel",
        "fcm",
        "firebase",
        "notifications"
    ],
    "authors": [
        {
            "name": "jasonviipers",
            "email": "jason@chatr.fr"
        }
    ],
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^6.2|^7.0",
        "illuminate/notifications": "^7.0|^8.0|^9.0",
        "illuminate/queue": "^7.0|^8.0|^9.0",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "illuminate/config": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^10.1",
        "orchestra/testbench": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Viipers\\FcmNotificationLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viipers\\FcmNotificationLaravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Viipers\\FcmNotificationLaravel\\FcmNotificationLaravelServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}